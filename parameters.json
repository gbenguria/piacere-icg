{
    "steps" : [{
        "programing_lenguage": "terraform",
        "output_path": "Output-code/",
        "data":{
            "provider": "aws",
            "network": [
                {
                    "id": 1,
                    "subname": "Danilo",
                    "vpcname": "Molteni"
                }
            ],
            "vm": [
                {
                    "id": 1,
                    "ram": 128,
                    "cpu": 32
                },
                {
                    "id": 2,
                    "ram": 256,
                    "cpu": 64
                }
            ],
            "db": [
                {
                    "id": 1,
                    "engine": "postgres",
                    "username": "danilo",
                    "storage": 1
                }
            ]
        }
    },{
        "programing_lenguage": "ansible",
        "type":"database",
        "output_path": "Output-code/",
        "info" : { 
            "name" : "postgres",
            "template_path" : "Databases-templates/"
        },
        "data":{
            "vars":{
                "DB_USER": "app1user",
                "DB_PASSWORD": "app1user",
                "DB_NAME": "app1"
            },
            "play":{
                "OS": "debian"
            }
        }
    },{
        "programing_lenguage": "ansible",
        "type":"docker-service",
        "output_path": "Output-code/",
        "info" : { 
            "name" : "wordpress",
            "template_path" : "Docker-services-templates/"
        },
        "data":{
            "vars": {
                "WORDPRESS_DB_HOST": "10.10.10.10",
                "WORDPRESS_DB_USER": "app1user",
                "WORDPRESS_DB_PASSWORD": "app1user",
                "WORDPRESS_DB_NAME": "app1",
                "WORDPRESS_TABLE_PREFIX": "wp"
            },
            "play":{
                "OS": "debian"
            }
        }
    }]
}